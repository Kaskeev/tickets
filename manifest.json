{
  "manifest_version": 3,
  "name": "Closer",
  "description": "Send Ticket on Gmail",  
  "browser_action": {
   "default_icon": "Closer.png",  
   "default_popup": "index.html"
  },
   "permissions": [
    "alarms",
    "tabs",
    "webNavigation",
    "*://*.google.com/"
   ],
   "icons": {
    "128": "Closer.png"
  },
  "version": "4.4.0"
}